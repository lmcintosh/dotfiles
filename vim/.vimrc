set tabstop=4
set shiftwidth=4
set expandtab
set softtabstop=4
set autoindent
set backspace=indent,eol,start
set number

set cindent
set cinkeys-=0#
set indentkeys-=#

set hlsearch
set incsearch
set showmatch

colorscheme slate

set guifont=Andale_Mono:h16

execute pathogen#infect()
syntax on
filetype plugin indent on
